{
  "name": "handlers",
  "version": "1.0.0",
  "license": "ISC",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx parcel index.html",
    "build": "parcel build ./src/html/* --no-cache --public-url /",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "re": "git add . && git commit -m main && git push origin main && npm run predeploy && npm run deploy"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "parcel": "^2.16.0"
  }
}
